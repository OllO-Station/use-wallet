    "./useConnectedWallet.js": {
      "import": "./useConnectedWallet.js",
      "require": "./_commonjs/useConnectedWallet.js",
      "node": "./_commonjs/useConnectedWallet.js",
      "default": "./useConnectedWallet.js"
    },
    "./useInstallableWallets": {
      "import": "./useInstallableWallets.js",
