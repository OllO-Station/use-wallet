    "./useLCDClient.js": {
      "import": "./useLCDClient.js",
      "require": "./_commonjs/useLCDClient.js",
      "node": "./_commonjs/useLCDClient.js",
      "default": "./useLCDClient.js"
    },
    "./useWallet": {
      "import": "./useWallet.js",
      "require": "./_commonjs/useWallet.js",
      "node": "./_commonjs/useWallet.js",
      "default": "./useWallet.js"
    },
    "./useWallet.js": {
      "import": "./useWallet.js",
      "require": "./_commonjs/useWallet.js",
      "node": "./_commonjs/useWallet.js",
      "default": "./useWallet.js"
    }
  },
  "description": "Core interface of [`@nestwallet/wallet-provider`](https://www.npmjs.com/package/@nestwallet/wallet-provider).",
